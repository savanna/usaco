{
  "configurations": {
    "Launch": {
      "adapter": "vscode-cpptools",
      "filetypes": [ "cpp", "c", "rust" ],
      "configuration": {
        "program": "${workspaceRoot}/test",

        "type": "cppdbg",
        "cwd": "${workspaceRoot}",
        "stopAtEntr": true,
        "request": "launch",
        "MIMode": "gdb",
         "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    }
  }
}
